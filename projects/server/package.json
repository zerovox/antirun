{
  "name": "@tsm/server",
  "version": "1.0.0",
  "main": "index.js",
  "bin": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p .",
    "start": "node ./dist/index.js",
    "lint": "tslint --fix 'src/**/*.ts*'",
    "build": "yarn compile && yarn package",
    "package": "pkg . -o bin/antirun -t node8-macos-x64,node8-linux-x64"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/knex": "^0.0.68",
    "@types/ws": "^3.2.1",
    "pkg": "^4.3.0-beta.5",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@tsm/app": "*",
    "@tsm/shared": "*",
    "express": "^4.16.2",
    "knex": "^0.14.2",
    "objection": "^0.9.4",
    "sqlite3": "^3.1.13",
    "ws": "^3.3.3"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "assets/**/*"
  }
